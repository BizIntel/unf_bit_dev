	// Многострочные части таблицы 7.
	ДанныеРаздела2_7 = мДанныеОтчета.ПолеТабличногоДокументаРаздел2_7;
	
	ДанныеПП = Новый Структура;
	ДанныеПП.Вставить("ДанныеМСЧ", ДанныеРаздела2_7);
	ДанныеПП.Вставить("ИмяРаздела", "Раздел2_7");
