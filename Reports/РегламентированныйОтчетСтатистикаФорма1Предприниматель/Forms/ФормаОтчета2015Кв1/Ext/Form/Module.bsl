&НаСервере
Функция СформироватьСтруктуруПараметров() Экспорт 
	
	Перем ПолученноеЗначение;
	
	СформироватьСтруктуруПоказателей();
	ТабДокумент = мДанныеОтчета.ПолеТабличногоДокументаФормаОтчета;
	ДопАтрибуты = РегламентированнаяОтчетность.СформироватьСтруктуруДопАтрибутов(ЭтаФорма.ИмяФормы, "АтрибВыгрузкиXML2015Кв1");
	
	СтрокаСведений = "ФИОРук";
	СведенияОбОрганизации = РегламентированнаяОтчетностьВызовСервера.ПолучитьСведенияОбОрганизации(СтруктураРеквизитовФормы.Организация, СтруктураРеквизитовФормы.мСохраненныйДок.ДатаПодписи, СтрокаСведений);
	
	ПараметрыВыгрузки = Новый Структура;
	
	ДопАтрибуты.Свойство("code", ПолученноеЗначение);
	ПараметрыВыгрузки.Вставить("КодШаблона", ПолученноеЗначение);
	ДопАтрибуты.Свойство("idf", ПолученноеЗначение);
	ПараметрыВыгрузки.Вставить("КодФормы", ПолученноеЗначение);
	ДопАтрибуты.Свойство("shifr", ПолученноеЗначение);
	ПараметрыВыгрузки.Вставить("ШифрФормы", ПолученноеЗначение);
	ДопАтрибуты.Свойство("version", ПолученноеЗначение);
	ПараметрыВыгрузки.Вставить("ВерсияШаблона", ПолученноеЗначение);
	ДопАтрибуты.Свойство("format_version", ПолученноеЗначение);
	ПараметрыВыгрузки.Вставить("ВерсияФормата", ПолученноеЗначение);
	
	ОтчПериод  = "01";
	РасчПериод = Формат(Год(СтруктураРеквизитовФормы.мСохраненныйДок.ДатаОкончания),"ЧГ=0");
	
	ПараметрыВыгрузки.Вставить("ИНН", СокрЛП(ТабДокумент.ИНН));
	ПараметрыВыгрузки.Вставить("ОКПО", СокрЛП(ТабДокумент.ОргКодОКПО));
	ПараметрыВыгрузки.Вставить("ОГРНИП", СокрЛП(ТабДокумент.ОГРН));
	ПараметрыВыгрузки.Вставить("ОтчПериод", СокрЛП(ОтчПериод));
	ПараметрыВыгрузки.Вставить("РасчПериод", СокрЛП(РасчПериод));
	ПараметрыВыгрузки.Вставить("ОргНазв", СокрЛП(ТабДокумент.ОргНазв));
	ПараметрыВыгрузки.Вставить("_ОргАдрес", РегламентированнаяОтчетностьКлиентСервер.ПредставлениеАдресаВФормате9Запятых(СтруктураРеквизитовФормы.мАдресвФорматеДляВыгрузки1));
	ПараметрыВыгрузки.Вставить("_ОргАдрес2", РегламентированнаяОтчетностьКлиентСервер.ПредставлениеАдресаВФормате9Запятых(СтруктураРеквизитовФормы.мАдресвФорматеДляВыгрузки2));
	ПараметрыВыгрузки.Вставить("ОргИсполнитель", СокрЛП(ТабДокумент.ОргИсполнитель));
	ПараметрыВыгрузки.Вставить("ОргТелефонИсп", СокрЛП(ТабДокумент.ОргТелефонИсп));
	ПараметрыВыгрузки.Вставить("ОргEmailИсп", СокрЛП(ТабДокумент.ОргАдресЭлектроннойПочты));
	ПараметрыВыгрузки.Вставить("ДатаСоставления", Формат(ТабДокумент.ДатаСоставленияОтчета, "ДФ=dd-MM-yyyy"));
	
	ПараметрыВыгрузки.Вставить("ПП51", ?(ЗначениеЗаполнено(ТабДокумент.П51), 1, 0));
	ПараметрыВыгрузки.Вставить("ПП52", ?(ЗначениеЗаполнено(ТабДокумент.П52), 1, 0));
	ПараметрыВыгрузки.Вставить("ПП53", ?(ЗначениеЗаполнено(ТабДокумент.П53), 1, 0));
	Если ЗначениеЗаполнено(ТабДокумент.Да7) Тогда 
		ПараметрыВыгрузки.Вставить("ПП71", 1);
	ИначеЕсли ЗначениеЗаполнено(ТабДокумент.Нет7) Тогда 
		ПараметрыВыгрузки.Вставить("ПП71", 2);
	Иначе 
		ПараметрыВыгрузки.Вставить("ПП71", 0);
	КонецЕсли;
	
	Если ЗначениеЗаполнено(ТабДокумент.Да8) Тогда 
		ПараметрыВыгрузки.Вставить("ПП81", 1);
	ИначеЕсли ЗначениеЗаполнено(ТабДокумент.Нет8) Тогда 
		ПараметрыВыгрузки.Вставить("ПП81", 2);
	Иначе 
		ПараметрыВыгрузки.Вставить("ПП81", 0);
	КонецЕсли;
	
	Если ЗначениеЗаполнено(ТабДокумент.Да88) Тогда 
		ПараметрыВыгрузки.Вставить("ПП88", 1);
	ИначеЕсли ЗначениеЗаполнено(ТабДокумент.Нет88) Тогда 
		ПараметрыВыгрузки.Вставить("ПП88", 2);
	Иначе
		ПараметрыВыгрузки.Вставить("ПП88", 0);
	КонецЕсли;
	
	ПараметрыВыгрузки.Вставить("ПП82", ?(ЗначениеЗаполнено(ТабДокумент.П82), 1, 0));
	ПараметрыВыгрузки.Вставить("ПП83", ?(ЗначениеЗаполнено(ТабДокумент.П83), 1, 0));
	ПараметрыВыгрузки.Вставить("ПП84", ?(ЗначениеЗаполнено(ТабДокумент.П84), 1, 0));
	ПараметрыВыгрузки.Вставить("ПП85", ?(ЗначениеЗаполнено(ТабДокумент.П85), 1, 0));
	ПараметрыВыгрузки.Вставить("ПП86", ?(ЗначениеЗаполнено(ТабДокумент.П86), 1, 0));
	
	ПараметрыВыгрузки.Вставить("ПП31", ?(ЗначениеЗаполнено(ТабДокумент.П31), 1, 0));
	ПараметрыВыгрузки.Вставить("ПП32", ?(ЗначениеЗаполнено(ТабДокумент.П32), 1, 0));
	ПараметрыВыгрузки.Вставить("ПП33", ?(ЗначениеЗаполнено(ТабДокумент.П33), 1, 0));
	ПараметрыВыгрузки.Вставить("ПП34", ?(ЗначениеЗаполнено(ТабДокумент.П34), 1, 0));
	ПараметрыВыгрузки.Вставить("ПП35", ?(ЗначениеЗаполнено(ТабДокумент.П35), 1, 0));
	
	Если ЗначениеЗаполнено(ТабДокумент.Да1) Тогда 
		ПараметрыВыгрузки.Вставить("ПП1", 1);
	ИначеЕсли ЗначениеЗаполнено(ТабДокумент.Нет1) Тогда 
		ПараметрыВыгрузки.Вставить("ПП1", 2);
	Иначе
		ПараметрыВыгрузки.Вставить("ПП1", 0);
	КонецЕсли;
	
	Если ЗначениеЗаполнено(ТабДокумент.Да11) Тогда 
		ПараметрыВыгрузки.Вставить("ПП11", 1);
	ИначеЕсли ЗначениеЗаполнено(ТабДокумент.Нет11) Тогда 
		ПараметрыВыгрузки.Вставить("ПП11", 2);
	Иначе
		ПараметрыВыгрузки.Вставить("ПП11", 0);
	КонецЕсли;
	
	Если ЗначениеЗаполнено(ТабДокумент.Да12) Тогда 
		ПараметрыВыгрузки.Вставить("ПП12", 1);
	ИначеЕсли ЗначениеЗаполнено(ТабДокумент.Нет12) Тогда 
		ПараметрыВыгрузки.Вставить("ПП12", 2);
	Иначе
		ПараметрыВыгрузки.Вставить("ПП12", 0);
	КонецЕсли;
	
	// Преобразование выгружаемых атрибутов, в соответствии
	// с форматом выгрузки статотчетности.
	ДопАтрибуты.Свойство("idp", ПолученноеЗначение);
	ПараметрыВыгрузки.Вставить("КодПериодичности", Число(СокрЛП(ПолученноеЗначение)));
	ПараметрыВыгрузки.Вставить("Документ", СтруктураРеквизитовФормы.мСохраненныйДок);
	
	РегламентированнаяОтчетность.АтрибутыВФорматеВыгрузки(ПараметрыВыгрузки);
	
	ПараметрыВыгрузки.Вставить("ИмяФайла", СформироватьИмяФайла(ПараметрыВыгрузки, ДопАтрибуты));
	
	ПараметрыВыгрузки.Вставить("ИмяКлючевогоУзлаСодержательнойЧасти", "row");
	
	Возврат ПараметрыВыгрузки;
	
КонецФункции

