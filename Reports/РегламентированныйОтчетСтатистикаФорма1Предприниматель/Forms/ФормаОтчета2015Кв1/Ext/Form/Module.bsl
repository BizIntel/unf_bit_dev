	СтруктураРеквизитовФормы.мПечатныеФормы.Добавить(ПечатнаяФорма, "Форма 1-предприниматель");
	
	Стр2 = ОбъектОтчета(ЭтаФорма.ИмяФормы).ПолучитьМакет("ПечатныйБланк_2015Кв1_Лист2");
	ПечатнаяФорма = РегламентированнаяОтчетность.НоваяСтраница();
	ПечатнаяФорма.ОриентацияСтраницы  = ОриентацияСтраницы.Портрет;
	ПечатнаяФорма.Вывести(Стр2);
	СтруктураРеквизитовФормы.мПечатныеФормы.Добавить(ПечатнаяФорма, "Форма 1-предприниматель");
	
	Стр3 = ОбъектОтчета(ЭтаФорма.ИмяФормы).ПолучитьМакет("ПечатныйБланк_2015Кв1_Лист3");
	ПечатнаяФорма = РегламентированнаяОтчетность.НоваяСтраница();
	ПечатнаяФорма.ОриентацияСтраницы  = ОриентацияСтраницы.Портрет;
	ПечатнаяФорма.Вывести(Стр3);
	ПечатнаяФорма.Области.Да1.Текст = ПолеТабличногоДокументаФормаОтчета.Области.Да1.Значение;
	ПечатнаяФорма.Области.Да11.Текст = ПолеТабличногоДокументаФормаОтчета.Области.Да11.Значение;
	ПечатнаяФорма.Области.Да12.Текст = ПолеТабличногоДокументаФормаОтчета.Области.Да12.Значение;
	ПечатнаяФорма.Области.Нет1.Текст = ПолеТабличногоДокументаФормаОтчета.Области.Нет1.Значение;
	ПечатнаяФорма.Области.Нет11.Текст = ПолеТабличногоДокументаФормаОтчета.Области.Нет11.Значение;
	ПечатнаяФорма.Области.Нет12.Текст = ПолеТабличногоДокументаФормаОтчета.Области.Нет12.Значение;
	ПечатнаяФорма.Области.ОргАдрес2.Текст = ПолеТабличногоДокументаФормаОтчета.Области.ОргАдрес2.Значение;
	ПечатнаяФорма.Области.П31.Текст = ПолеТабличногоДокументаФормаОтчета.Области.П31.Значение;
	ПечатнаяФорма.Области.П32.Текст = ПолеТабличногоДокументаФормаОтчета.Области.П32.Значение;
	ПечатнаяФорма.Области.П33.Текст = ПолеТабличногоДокументаФормаОтчета.Области.П33.Значение;
	ПечатнаяФорма.Области.П34.Текст = ПолеТабличногоДокументаФормаОтчета.Области.П34.Значение;
	ПечатнаяФорма.Области.П35.Текст = ПолеТабличногоДокументаФормаОтчета.Области.П35.Значение;
	Для Инд = 1 По 12 Цикл
		ПечатнаяФорма.Области["ИНН_"+Инд].Текст = Сред(ПолеТабличногоДокументаФормаОтчета.Области.ИНН.Значение, Инд, 1);
	КонецЦикла;
	
	ВывестиЧисло(ПечатнаяФорма, ПолеТабличногоДокументаФормаОтчета.Области.П13.Значение, "П13_", 2, 0, Ложь);
	ВывестиЧисло(ПечатнаяФорма, ПолеТабличногоДокументаФормаОтчета.Области.П41.Значение * 10, "П41_", 4, 1, Ложь);
	ВывестиЧисло(ПечатнаяФорма, ПолеТабличногоДокументаФормаОтчета.Области.П43.Значение * 10, "П43_", 4, 1, Ложь);
	ВывестиЧисло(ПечатнаяФорма, ПолеТабличногоДокументаФормаОтчета.Области.П44.Значение * 10, "П44_", 4, 1, Ложь);
	ВывестиЧисло(ПечатнаяФорма, ПолеТабличногоДокументаФормаОтчета.Области.П45.Значение * 10, "П45_", 4, 1, Ложь);
	СтруктураРеквизитовФормы.мПечатныеФормы.Добавить(ПечатнаяФорма, "Форма 1-предприниматель");
	
	Стр4 = ОбъектОтчета(ЭтаФорма.ИмяФормы).ПолучитьМакет("ПечатныйБланк_2015Кв1_Лист4");
	ПечатнаяФорма = РегламентированнаяОтчетность.НоваяСтраница();
	ПечатнаяФорма.ПолеСнизу = 1;
	ПечатнаяФорма.ОриентацияСтраницы  = ОриентацияСтраницы.Портрет;
	ПечатнаяФорма.Вывести(Стр4);
	ПечатнаяФорма.Области.Да7.Текст = ПолеТабличногоДокументаФормаОтчета.Области.Да7.Значение;
	Если ЗначениеЗаполнено(ПечатнаяФорма.Области.Да7.Текст) Тогда 
		ВывестиЧисло(ПечатнаяФорма, ПолеТабличногоДокументаФормаОтчета.Области.П73.Значение, "П73_", 6, 0);
		ВывестиЧисло(ПечатнаяФорма, ПолеТабличногоДокументаФормаОтчета.Области.П74.Значение, "П74_", 6, 0);
	КонецЕсли;
	ПечатнаяФорма.Области.Да8.Текст = ПолеТабличногоДокументаФормаОтчета.Области.Да8.Значение;
	ПечатнаяФорма.Области.Да88.Текст = ПолеТабличногоДокументаФормаОтчета.Области.Да88.Значение;
	ПечатнаяФорма.Области.Нет7.Текст = ПолеТабличногоДокументаФормаОтчета.Области.Нет7.Значение;
	ПечатнаяФорма.Области.Нет8.Текст = ПолеТабличногоДокументаФормаОтчета.Области.Нет8.Значение;
	ПечатнаяФорма.Области.Нет88.Текст = ПолеТабличногоДокументаФормаОтчета.Области.Нет88.Значение;
	ПечатнаяФорма.Области.П51.Текст = ПолеТабличногоДокументаФормаОтчета.Области.П51.Значение;
	ПечатнаяФорма.Области.П52.Текст = ПолеТабличногоДокументаФормаОтчета.Области.П52.Значение;
	ПечатнаяФорма.Области.П53.Текст = ПолеТабличногоДокументаФормаОтчета.Области.П53.Значение;
	ПечатнаяФорма.Области.П82.Текст = ПолеТабличногоДокументаФормаОтчета.Области.П82.Значение;
	ПечатнаяФорма.Области.П83.Текст = ПолеТабличногоДокументаФормаОтчета.Области.П83.Значение;
	ПечатнаяФорма.Области.П84.Текст = ПолеТабличногоДокументаФормаОтчета.Области.П84.Значение;
	ПечатнаяФорма.Области.П85.Текст = ПолеТабличногоДокументаФормаОтчета.Области.П85.Значение;
	ПечатнаяФорма.Области.П86.Текст = ПолеТабличногоДокументаФормаОтчета.Области.П86.Значение;
	
	ПечатнаяФорма.Области.П62_Наим_1.Текст = ПолеТабличногоДокументаФормаОтчета.Области.П000006000101_1.Значение;
	ПечатнаяФорма.Области.П62_Наим_2.Текст = ПолеТабличногоДокументаФормаОтчета.Области.П000006000101_2.Значение;
	ПечатнаяФорма.Области.П62_Наим_3.Текст = ПолеТабличногоДокументаФормаОтчета.Области.П000006000101_3.Значение;
	
	ВывестиЧисло(ПечатнаяФорма, ПолеТабличногоДокументаФормаОтчета.Области.П61.Значение*10, "П61_", 8, 1, Ложь);
	ВывестиЧисло(ПечатнаяФорма, ПолеТабличногоДокументаФормаОтчета.Области.П000006000104_1.Значение*10, "П62_1_", 8, 1, Ложь);
	ВывестиЧисло(ПечатнаяФорма, ПолеТабличногоДокументаФормаОтчета.Области.П000006000104_2.Значение*10, "П62_2_", 8, 1, Ложь);
	ВывестиЧисло(ПечатнаяФорма, ПолеТабличногоДокументаФормаОтчета.Области.П000006000104_3.Значение*10, "П62_3_", 8, 1, Ложь);
	
	ВывестиОКВЭД(ПечатнаяФорма, ПолеТабличногоДокументаФормаОтчета.Области.П000006000103_1.Значение, "П62_оквэд_1_");
	ВывестиОКВЭД(ПечатнаяФорма, ПолеТабличногоДокументаФормаОтчета.Области.П000006000103_2.Значение, "П62_оквэд_2_");
	ВывестиОКВЭД(ПечатнаяФорма, ПолеТабличногоДокументаФормаОтчета.Области.П000006000103_3.Значение, "П62_оквэд_3_");
	
	Для Инд = 1 По 20 Цикл
		ПечатнаяФорма.Области["ОргАдресЭлектроннойПочты_"+Инд].Текст = Сред(ПолеТабличногоДокументаФормаОтчета.Области.ОргАдресЭлектроннойПочты.Значение, Инд, 1);
	КонецЦикла;
	
	ОргТелефонИсп = ПолеТабличногоДокументаФормаОтчета.Области.ОргТелефонИсп.Значение;
	ОргТелефонИсп = СтрЗаменить(ОргТелефонИсп, "-", "");
	ОргТелефонИсп = СтрЗаменить(ОргТелефонИсп, "+", "");
	ОргТелефонИсп = СтрЗаменить(ОргТелефонИсп, "(", "");
	ОргТелефонИсп = СтрЗаменить(ОргТелефонИсп, ")", "");
	ОргТелефонИсп = СтрЗаменить(ОргТелефонИсп, " ", "");
	ОргТелефонИсп = Прав("                        " + ОргТелефонИсп, 10);
	Для Инд = 1 По 10 Цикл
		ПечатнаяФорма.Области["ОргТелефонИсп_"+Инд].Текст = Сред(ОргТелефонИсп, Инд, 1);
