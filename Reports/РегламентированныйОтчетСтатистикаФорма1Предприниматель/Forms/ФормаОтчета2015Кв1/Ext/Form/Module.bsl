	ПечатнаяФорма.Области.ФИО.Текст = ПолеТабличногоДокументаФормаОтчета.Области.ОргНазв.Значение;
	Для Инд = 1 По 12 Цикл
		ПечатнаяФорма.Области["ИНН_"+Инд].Текст = Сред(ПолеТабличногоДокументаФормаОтчета.Области.ИНН.Значение, Инд, 1);
	КонецЦикла;
	Для Инд = 1 По 15 Цикл
		ПечатнаяФорма.Области["ОГРНИП_"+Инд].Текст = Сред(ПолеТабличногоДокументаФормаОтчета.Области.ОГРН.Значение, Инд, 1);
