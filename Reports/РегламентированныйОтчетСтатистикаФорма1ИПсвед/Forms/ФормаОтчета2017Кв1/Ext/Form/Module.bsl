&НаСервере
Процедура ДополнительныеПоказатели1ИП(ТекСтруктура)
	
	Перем ТекЗначение100, ТекЗначение10, ТекЗначение;
	
	// Раздел 4.
	НомСтр = 1;
	Пока ТекСтруктура.Свойство("П000004000102_" + Формат(НомСтр, "ЧГ="), ТекЗначение) Цикл
		Если Не ЗначениеЗаполнено(ТекЗначение) Тогда
			НомСтр = НомСтр + 1;
			Продолжить;
		КонецЕсли;
		ФильтрованнаяТЗ = СвойстваПоказателей.НайтиСтроки(Новый Структура("Показатель", "П000004000102"))[0].ТаблицаЗначений.НайтиСтроки(Новый Структура("Название", ТекЗначение));
		// Следующее условие - на случай, если ТекЗначение внесено вручную
		// (а не выбором из справочника, согласно шаблону).
		Если ФильтрованнаяТЗ.Количество() > 0 Тогда
			ТекСтрокаТЗ     = ФильтрованнаяТЗ[ФильтрованнаяТЗ.Количество() - 1];
			Если ТекСтрокаТЗ <> Неопределено Тогда
				ТекСтруктура.Вставить("ПД00004000102_" + Формат(НомСтр, "ЧГ="), СокрЛП(ТекСтрокаТЗ.Код));
			Иначе
				ТекСтруктура.Вставить("ПД00004000102_" + Формат(НомСтр, "ЧГ="), "");
			КонецЕсли;
		КонецЕсли;
		НомСтр = НомСтр + 1;
	КонецЦикла;
	
	// Раздел 4.1: из строковых значений в "квадратах" получаем числа для каждой строки.
	НомСтр = 1;
	Пока  ТекСтруктура.Свойство("П000004000103_" + Формат(НомСтр, "ЧГ="), ТекЗначение100)
		И ТекСтруктура.Свойство("П000004000104_" + Формат(НомСтр, "ЧГ="), ТекЗначение10)
		И ТекСтруктура.Свойство("П000004000105_" + Формат(НомСтр, "ЧГ="), ТекЗначение) Цикл
		ТекСумма = 100 * Число(?(ТекЗначение100 = "", "0", ТекЗначение100))
				 +  10 * Число(?(ТекЗначение10  = "", "0", ТекЗначение10))
				 +       Число(?(ТекЗначение    = "", "0", ТекЗначение));
		ТекСтруктура.Вставить("ПД00104000101_" + Формат(НомСтр, "ЧГ="), ТекСумма);
		НомСтр = НомСтр + 1;
	КонецЦикла;
	
Конецпроцедуры

&НаСервере
Функция СформироватьСтруктуруПараметров() Экспорт 
	
	Перем ПолученноеЗначение;
	
	СформироватьСтруктуруПоказателей();
	ТабДокумент = мДанныеОтчета.ПолеТабличногоДокументаФормаОтчета;
	ДопАтрибуты = РегламентированнаяОтчетность.СформироватьСтруктуруДопАтрибутов(ЭтаФорма.ИмяФормы, "АтрибВыгрузкиXML2017Кв1");
	
	СтрокаСведений = "ФИОРук";
	СведенияОбОрганизации = РегламентированнаяОтчетностьВызовСервера.ПолучитьСведенияОбОрганизации(СтруктураРеквизитовФормы.Организация, СтруктураРеквизитовФормы.мСохраненныйДок.ДатаПодписи, СтрокаСведений);
	
	ПараметрыВыгрузки = Новый Структура;
	
	ДопАтрибуты.Свойство("code", ПолученноеЗначение);
	ПараметрыВыгрузки.Вставить("КодШаблона", ПолученноеЗначение);
	ДопАтрибуты.Свойство("idf", ПолученноеЗначение);
	ПараметрыВыгрузки.Вставить("КодФормы", ПолученноеЗначение);
	ДопАтрибуты.Свойство("shifr", ПолученноеЗначение);
	ПараметрыВыгрузки.Вставить("ШифрФормы", ПолученноеЗначение);
	ДопАтрибуты.Свойство("version", ПолученноеЗначение);
	ПараметрыВыгрузки.Вставить("ВерсияШаблона", ПолученноеЗначение);
	ДопАтрибуты.Свойство("format_version", ПолученноеЗначение);
	ПараметрыВыгрузки.Вставить("ВерсияФормата", ПолученноеЗначение);
	
	// Возможное значение отчетного периода, согласно шаблону Росстата: 12.
	ОтчПериод  = 12;
	РасчПериод = Формат(Год(СтруктураРеквизитовФормы.мСохраненныйДок.ДатаОкончания),"ЧГ=0");
	
	ПараметрыВыгрузки.Вставить("ИНН", СокрЛП(ТабДокумент.ИНН));
	ПараметрыВыгрузки.Вставить("ОКПО", СокрЛП(ТабДокумент.ОргКодОКПО));
	ПараметрыВыгрузки.Вставить("ОтчПериод", СокрЛП(ОтчПериод));
	ПараметрыВыгрузки.Вставить("РасчПериод", СокрЛП(РасчПериод));
	ПараметрыВыгрузки.Вставить("ОргНазв", СокрЛП(ТабДокумент.ОргНазв));
	ПараметрыВыгрузки.Вставить("ОргДиректор", СокрЛП(СведенияОбОрганизации.ФИОРук));
	ПараметрыВыгрузки.Вставить("ОргДолжностьИсп", СокрЛП(ТабДокумент.ОргДолжностьИсп));
	ПараметрыВыгрузки.Вставить("ОргИсполнитель", СокрЛП(ТабДокумент.ОргИсполнитель));
	ПараметрыВыгрузки.Вставить("ОргТелефонИсп", СокрЛП(ТабДокумент.ОргТелефонИсп));
	ПараметрыВыгрузки.Вставить("ОргЭлектроннаяПочта", СокрЛП(ТабДокумент.ОргАдресЭлектроннойПочты));
	
	// Преобразование выгружаемых атрибутов, в соответствии
	// с форматом выгрузки статотчетности.
	ДопАтрибуты.Свойство("idp", ПолученноеЗначение);
	ПараметрыВыгрузки.Вставить("КодПериодичности", Число(СокрЛП(ПолученноеЗначение)));
	ПараметрыВыгрузки.Вставить("Документ", СтруктураРеквизитовФормы.мСохраненныйДок);
	
	РегламентированнаяОтчетность.АтрибутыВФорматеВыгрузки(ПараметрыВыгрузки);
	
	ПараметрыВыгрузки.Вставить("ИмяФайла", СформироватьИмяФайла(ПараметрыВыгрузки, ДопАтрибуты));
	
	ПараметрыВыгрузки.Вставить("ИмяКлючевогоУзлаСодержательнойЧасти", "row");
	
	ПараметрыВыгрузки.Вставить("Зн101", ?(ЗначениеЗаполнено(ТабДокумент.П000001000101), 1, 0));
	ПараметрыВыгрузки.Вставить("Зн102", ?(ЗначениеЗаполнено(ТабДокумент.П000001000201), 1, 0));
	ПараметрыВыгрузки.Вставить("Зн111", ?(ЗначениеЗаполнено(ТабДокумент.П000101000101), 1, 0));
	ПараметрыВыгрузки.Вставить("Зн112", ?(ЗначениеЗаполнено(ТабДокумент.П000101000201), 1, 0));
	ПараметрыВыгрузки.Вставить("Зн201", ?(ЗначениеЗаполнено(ТабДокумент.П000002000101), 1, 0));
	ПараметрыВыгрузки.Вставить("Зн202", ?(ЗначениеЗаполнено(ТабДокумент.П000002000201), 1, 0));
	
	// Для ПОСЛЕДУЮЩЕГО (ПЕРЕД ВЫГРУЗКОЙ) контроля показателей формы (Указания ФСН по заполнению Формы 1-ИП),
	// а также для ВЫГРУЗКИ:
	
	// Раздел 1.
	// Для контроля.
	ПараметрыВыгрузки.Вставить("П000001000101", ?(ЗначениеЗаполнено(ТабДокумент["П000001000101"]), 1, 2));
	ПараметрыВыгрузки.Вставить("П000001000201", ?(ЗначениеЗаполнено(ТабДокумент["П000001000201"]), 1, 2));
	// Для выгрузки.
	Если ПараметрыВыгрузки["П000001000101"] = 1 Тогда
		ПараметрыВыгрузки.Вставить("ПЧ00001000101", 1);
	ИначеЕсли ПараметрыВыгрузки["П000001000201"] = 1 Тогда
		ПараметрыВыгрузки.Вставить("ПЧ00001000101", 2);
	Иначе
		ПараметрыВыгрузки.Вставить("ПЧ00001000101", "");
	КонецЕсли;
	
	// Раздел 1.1.
	// Для контроля.
	ПараметрыВыгрузки.Вставить("П000101000101", ?(ЗначениеЗаполнено(ТабДокумент["П000101000101"]), 1, 2));
	ПараметрыВыгрузки.Вставить("П000101000201", ?(ЗначениеЗаполнено(ТабДокумент["П000101000201"]), 1, 2));
	// Для выгрузки.
	Если ПараметрыВыгрузки["П000101000101"] = 1 Тогда
		ПараметрыВыгрузки.Вставить("ПЧ00101000101", 1);
	ИначеЕсли ПараметрыВыгрузки["П000101000201"] = 1 Тогда
		ПараметрыВыгрузки.Вставить("ПЧ00101000101", 2);
	Иначе
		ПараметрыВыгрузки.Вставить("ПЧ00101000101", "");
	КонецЕсли;
	
	// Раздел 2.
	// Для контроля.
	ПараметрыВыгрузки.Вставить("П000002000101", ?(ЗначениеЗаполнено(ТабДокумент["П000002000101"]), 1, 2));
	ПараметрыВыгрузки.Вставить("П000002000201", ?(ЗначениеЗаполнено(ТабДокумент["П000002000201"]), 1, 2));
	// Для выгрузки.
	Если ПараметрыВыгрузки["П000002000101"] = 1 Тогда
		ПараметрыВыгрузки.Вставить("ПЧ00002000101", 1);
	ИначеЕсли ПараметрыВыгрузки["П000002000201"] = 1 Тогда
		ПараметрыВыгрузки.Вставить("ПЧ00002000101", 2);
	Иначе
		ПараметрыВыгрузки.Вставить("ПЧ00002000101", "");
	КонецЕсли;
	Попытка
		ТекСтрокаТЗ = СвойстваПоказателей.НайтиСтроки(Новый Структура("Показатель", "П000002000301"))[0].ТаблицаЗначений.НайтиСтроки(Новый Структура("Название", ТабДокумент["П000002000301"]))[0];
		ПараметрыВыгрузки.Вставить("ПЧ00002000301", Число(ТекСтрокаТЗ.Код));
	Исключение
		ПараметрыВыгрузки.Вставить("ПЧ00002000301", "");
	КонецПопытки;
	
	// Раздел 3.
	// Для контроля.
	ПараметрыВыгрузки.Вставить("П000003000101", ТабДокумент["П000003000101"]);
	
	// Раздел 4.
	// Для контроля.
	ТекСумма = 0;
	НомСтр = 1;
	Пока РегламентированнаяОтчетностьКлиентСервер.СвойствоОпределено(ТабДокумент, "П000004000103_" + Формат(НомСтр, "ЧГ=")) Цикл
		ТекСумма = 100 * Число(?(ТабДокумент["П000004000103_" + Формат(НомСтр, "ЧГ=")] = "", "0", ТабДокумент["П000004000103_" + Формат(НомСтр, "ЧГ=")]))
				 +  10 * Число(?(ТабДокумент["П000004000104_" + Формат(НомСтр, "ЧГ=")] = "", "0", ТабДокумент["П000004000104_" + Формат(НомСтр, "ЧГ=")]))
				 +       Число(?(ТабДокумент["П000004000105_" + Формат(НомСтр, "ЧГ=")] = "", "0", ТабДокумент["П000004000105_" + Формат(НомСтр, "ЧГ=")]));
		ПараметрыВыгрузки.Вставить("ПЧ00104000101_" + Формат(НомСтр, "ЧГ="), ТекСумма);
		НомСтр = НомСтр + 1;
	КонецЦикла;
	
	Возврат ПараметрыВыгрузки;
	
КонецФункции

