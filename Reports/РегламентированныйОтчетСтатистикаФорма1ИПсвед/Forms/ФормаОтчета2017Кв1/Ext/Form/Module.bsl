	ПолеТабличногоДокументаФормаОтчета.Области["П000004000104_" + Суффикс].Значение = Цел(ПервоначальноеЗначение / 10);
	ПолеТабличногоДокументаФормаОтчета.Области["П000004000105_" + Суффикс].Значение = Цел(ПервоначальноеЗначение - Число(ПолеТабличногоДокументаФормаОтчета.Области["П000004000104_" + Суффикс].Значение) * 10);
	ПолеТабличногоДокументаФормаОтчета.Области["П000004000104_" + Суффикс].Значение = ?(ПервоначальноеЗначение <  10, "", ПолеТабличногоДокументаФормаОтчета.Области["П000004000104_" + Суффикс].Значение);
	ПолеТабличногоДокументаФормаОтчета.Области["П000004000105_" + Суффикс].Значение = ?(ПервоначальноеЗначение <   1, "", ПолеТабличногоДокументаФормаОтчета.Области["П000004000105_" + Суффикс].Значение);
	
