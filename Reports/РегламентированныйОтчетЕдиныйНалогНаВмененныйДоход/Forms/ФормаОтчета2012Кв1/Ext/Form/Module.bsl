&НаСервере
Процедура ПроверкаКС_1_4(СтруктураРезультата)

	ОбщееЧислоОшибок = СтруктураРезультата.КоличествоНайденныхОшибок; // Счетчик учета ошибок.
	ОбщееЧислоБезОшибок = СтруктураРезультата.КоличествоНайденныхБезОшибок; // Счетчик учета проверенных соотношений, не являющиеся ошибками.
	СтруктураОшибок = СтруктураРезультата.СтруктураОшибок;
	
	ТекстФормулы = "";
	
	НомерСтраницы = 1;
	
	Для каждого Страница Из СтруктураРеквизитовФормы.мДанныеОтчета.Раздел2 Цикл
				
		ОписаниеКС = "Сумма налоговой базы по ЕНВД (строка 100 Раздела 2) должна быть равна произведению базовой доходности с учетом корректирующих коэффициентов и суммы величин физических показателей за отчетный период (строка 040 * строка 080 * строка 090 * (строка 050 + строка 060 + строка 070)) по одноименному ОКАТО (строка 030)";
		
		СчетчикФормулДляРасшифровок = СчетчикФормулДляРасшифровок + 1;
		РегламентированнаяОтчетность.ЗарегистрироватьФормулуДляРасшифровки(СписокРасшифровкиФормулы, ОписаниеКС, 
			, СчетчикФормулДляРасшифровок);
		
		// Зарегистрируем левую часть соотношения.
		СуммаР2Стр100 = РегламентированнаяОтчетностьКлиентСервер.ОкруглитьЧислоПоФормату(ЭтаФорма, Страница.Данные[0].Значение.П000020010003);
		НазваниеПоказателя = "Раздел 2, строка 100, страница № " + НомерСтраницы;
		РегламентированнаяОтчетность.ЗарегистрироватьПоказательДляРасшифровки(СписокРасшифровкиПоказателей, "П000020010003",
			НазваниеПоказателя, СуммаР2Стр100, "", СтруктураРеквизитовФормы.мСохраненныйДок.Ссылка, "Раздел2", Неопределено,
			НомерСтраницы, СчетчикФормулДляРасшифровок);
		СуммаР2Стр100Стр = Формат(СуммаР2Стр100, "ЧРД=.;ЧН=0");
		
		// Зарегистрируем правую часть соотношения.
		СуммаР2Стр040 = РегламентированнаяОтчетностьКлиентСервер.ОкруглитьЧислоПоФормату(ЭтаФорма, Страница.Данные[0].Значение.П000020004003);
		НазваниеПоказателя = "Раздел 2, строка 040, страница № " + НомерСтраницы;
		РегламентированнаяОтчетность.ЗарегистрироватьПоказательДляРасшифровки(СписокРасшифровкиПоказателей, "П000020004003",
			НазваниеПоказателя, СуммаР2Стр040, "", СтруктураРеквизитовФормы.мСохраненныйДок.Ссылка, "Раздел2", Неопределено,
			НомерСтраницы, СчетчикФормулДляРасшифровок);
		СуммаР2Стр040Стр = Формат(СуммаР2Стр040, "ЧРД=.;ЧН=0");
		
		СуммаР2Стр080 = Страница.Данные[0].Значение.П000020008003; // коэффициенты не округляем
		НазваниеПоказателя = "Раздел 2, строка 080, страница № " + НомерСтраницы;
		РегламентированнаяОтчетность.ЗарегистрироватьПоказательДляРасшифровки(СписокРасшифровкиПоказателей, "П000020008003",
			НазваниеПоказателя, СуммаР2Стр080, "", СтруктураРеквизитовФормы.мСохраненныйДок.Ссылка, "Раздел2", Неопределено,
			НомерСтраницы, СчетчикФормулДляРасшифровок);
		СуммаР2Стр080Стр = Формат(СуммаР2Стр080, "ЧРД=.;ЧН=0");
		
		СуммаР2Стр090 = Страница.Данные[0].Значение.П000020009003; // коэффициенты не округляем
		НазваниеПоказателя = "Раздел 2, строка 090, страница № " + НомерСтраницы;
		РегламентированнаяОтчетность.ЗарегистрироватьПоказательДляРасшифровки(СписокРасшифровкиПоказателей, "П000020009003",
			НазваниеПоказателя, СуммаР2Стр090, "", СтруктураРеквизитовФормы.мСохраненныйДок.Ссылка, "Раздел2", Неопределено,
			НомерСтраницы, СчетчикФормулДляРасшифровок);
		СуммаР2Стр090Стр = Формат(СуммаР2Стр090, "ЧРД=.;ЧН=0");
		
		СуммаР2Стр050 = РегламентированнаяОтчетностьКлиентСервер.ОкруглитьЧислоПоФормату(ЭтаФорма, Страница.Данные[0].Значение.П000020005003);
		НазваниеПоказателя = "Раздел 2, строка 050, страница № " + НомерСтраницы;
		РегламентированнаяОтчетность.ЗарегистрироватьПоказательДляРасшифровки(СписокРасшифровкиПоказателей, "П000020005003",
			НазваниеПоказателя, СуммаР2Стр050, "", СтруктураРеквизитовФормы.мСохраненныйДок.Ссылка, "Раздел2", Неопределено,
			НомерСтраницы, СчетчикФормулДляРасшифровок);
		СуммаР2Стр050Стр = Формат(СуммаР2Стр050, "ЧРД=.;ЧН=0");
		
		СуммаР2Стр060 = РегламентированнаяОтчетностьКлиентСервер.ОкруглитьЧислоПоФормату(ЭтаФорма, Страница.Данные[0].Значение.П000020006003);
		НазваниеПоказателя = "Раздел 2, строка 060, страница № " + НомерСтраницы;
		РегламентированнаяОтчетность.ЗарегистрироватьПоказательДляРасшифровки(СписокРасшифровкиПоказателей, "П000020006003",
			НазваниеПоказателя, СуммаР2Стр060, "", СтруктураРеквизитовФормы.мСохраненныйДок.Ссылка, "Раздел2", Неопределено,
			НомерСтраницы, СчетчикФормулДляРасшифровок);
		СуммаР2Стр060Стр = Формат(СуммаР2Стр060, "ЧРД=.;ЧН=0");
		
		СуммаР2Стр070 = РегламентированнаяОтчетностьКлиентСервер.ОкруглитьЧислоПоФормату(ЭтаФорма, Страница.Данные[0].Значение.П000020007003);
		НазваниеПоказателя = "Раздел 2, строка 070, страница № " + НомерСтраницы;
		РегламентированнаяОтчетность.ЗарегистрироватьПоказательДляРасшифровки(СписокРасшифровкиПоказателей, "П000020007003",
			НазваниеПоказателя, СуммаР2Стр070, "", СтруктураРеквизитовФормы.мСохраненныйДок.Ссылка, "Раздел2", Неопределено,
			НомерСтраницы, СчетчикФормулДляРасшифровок);
		СуммаР2Стр070Стр = Формат(СуммаР2Стр070, "ЧРД=.;ЧН=0");
		
		ЛеваяЧасть = СуммаР2Стр100;
		ЛеваяЧастьСтр = СуммаР2Стр100Стр;
		
		ПраваяЧасть = (СуммаР2Стр040 * СуммаР2Стр080 * СуммаР2Стр090) * (СуммаР2Стр050 + СуммаР2Стр060 + СуммаР2Стр070);
		ПраваяЧасть = РегламентированнаяОтчетностьКлиентСервер.ОкруглитьЧислоПоФормату(ЭтаФорма, ПраваяЧасть);
		ПраваяЧастьСтр = Формат(ПраваяЧасть, "ЧРД=.;ЧН=0");
		
		Если ЛеваяЧасть = ПраваяЧасть Тогда 
			ЭтоОшибка = Ложь;
			ОбщееЧислоБезОшибок = ОбщееЧислоБезОшибок + 1;
		Иначе
			ЭтоОшибка = Истина;
			ОбщееЧислоОшибок = ОбщееЧислоОшибок + 1;
		КонецЕсли;
		
		Расшифровка = ЛеваяЧастьСтр + " = " + СуммаР2Стр040Стр + " * " + СуммаР2Стр080Стр + " * "+ СуммаР2Стр090Стр + " * ("
		+ СуммаР2Стр050Стр + " + " + СуммаР2Стр060Стр + " + " + СуммаР2Стр070Стр + ") Итог [" + ПраваяЧастьСтр + "]";
		
		ТекстОписания = "Неправильно исчислена налоговая база за налоговый период.";
		Если ЭтоОшибка Тогда 
			ТекстОписания = ТекстОписания
			+ Символы.ПС 
			+ "Исключение: Данное контрольное соотношение не применяется, если в течение квартала были произведены постановка или снятие с учета в налоговом органе."
		КонецЕсли;
			
		ТекстовоеПредставлениеФормулы = ?(ПустаяСтрока(ТекстФормулы), "", ТекстФормулы + ". ") + ОписаниеКС;
		СтруктураОшибок.Вставить("Описание" + Формат(ОбщееЧислоОшибок + ОбщееЧислоБезОшибок, "ЧГ=0"), ТекстОписания);
		СтруктураОшибок.Вставить("Нарушение" + Формат(ОбщееЧислоОшибок + ОбщееЧислоБезОшибок, "ЧГ=0"), "п.п. 2, 4 ст.346.29 НК РФ");
		СтруктураОшибок.Вставить("Расшифровка" + Формат(ОбщееЧислоОшибок + ОбщееЧислоБезОшибок, "ЧГ=0"), Расшифровка);
		СтруктураОшибок.Вставить("НомерФормулыРасшифровки" + Формат(ОбщееЧислоОшибок + ОбщееЧислоБезОшибок, "ЧГ=0"), СчетчикФормулДляРасшифровок);
		СтруктураОшибок.Вставить("ЭтоОшибка" + Формат(ОбщееЧислоОшибок + ОбщееЧислоБезОшибок, "ЧГ=0"), ЭтоОшибка);
		СтруктураОшибок.Вставить("ТекстовоеПредставлениеФормулы" + Формат(ОбщееЧислоОшибок + ОбщееЧислоБезОшибок, "ЧГ=0"), ТекстовоеПредставлениеФормулы);
		
		НомерСтраницы = НомерСтраницы + 1;
		
