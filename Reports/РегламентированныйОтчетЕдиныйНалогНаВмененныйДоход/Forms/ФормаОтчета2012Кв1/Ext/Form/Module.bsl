&НаСервере
Функция ПроверитьКонтрольныеСоотношенияВОтчете() Экспорт
	
	СформироватьСтруктуруПоказателей();
			
	СтруктураРеквизитовФормы.ТекстДляЗаголовочнойЧасти = РегламентированнаяОтчетностьКлиентСервер.ПредставлениеДокументаРеглОтч(СтруктураРеквизитовФормы.мСохраненныйДок);
	
	СтруктураРезультата = Новый Структура;
	СтруктураРезультата.Вставить("КоличествоНайденныхОшибок",    0);
	СтруктураРезультата.Вставить("КоличествоНайденныхБезОшибок", 0);
	СтруктураРезультата.Вставить("СтруктураОшибок", Неопределено);
	
	Если СписокРасшифровкиПоказателей = Неопределено Тогда 
		СписокРасшифровкиПоказателей = РегламентированнаяОтчетность.СоздатьСписокРасшифровкиПоказателей();
	Иначе
		СписокРасшифровкиПоказателей.Очистить();
	КонецЕсли;
	
	Если СписокРасшифровкиформулы = Неопределено Тогда 
		СписокРасшифровкиформулы = РегламентированнаяОтчетность.СоздатьСписокРасшифровкиформулы();
	Иначе
		СписокРасшифровкиформулы.Очистить();
	КонецЕсли;
	
	СчетчикФормулДляРасшифровок = 0;
	
	ПроверкаКС_1_1(СтруктураРезультата);
	ПроверкаКС_1_2(СтруктураРезультата);
	ПроверкаКС_1_3(СтруктураРезультата);
	ПроверкаКС_1_4(СтруктураРезультата);
	ПроверкаКС_1_5(СтруктураРезультата);
	ПроверкаКС_1_6(СтруктураРезультата);
	ПроверкаКС_1_7(СтруктураРезультата);
	ПроверкаКС_1_8(СтруктураРезультата);
	ПроверкаКС_1_9(СтруктураРезультата);
	ПроверкаКС_1_10(СтруктураРезультата);
	
	Если СписокРасшифровкиПоказателей.Количество() > 0 Тогда 
		СтруктураРезультата.Вставить("СписокРасшифровкиПоказателей", 
			ПоместитьВоВременноеХранилище(СписокРасшифровкиПоказателей, Новый УникальныйИдентификатор));
	КонецЕсли;
	
	Если СписокРасшифровкиформулы.Количество() > 0 Тогда 
		СтруктураРезультата.Вставить("СписокРасшифровкиформулы", 
			ПоместитьВоВременноеХранилище(СписокРасшифровкиформулы, Новый УникальныйИдентификатор));
	КонецЕсли;
	
	Возврат СтруктураРезультата;
	
КонецФункции

&НаСервере
Процедура ПроверкаКС_1_1(СтруктураРезультата)

	ОбщееЧислоОшибок = 0; // Счетчик учета ошибок.
	ОбщееЧислоБезОшибок = 0; // Счетчик учета проверенных соотношений, не являющиеся ошибками.

	СтруктураОшибок = Новый Структура;
	
	ТекстФормулы = "";
	
	СтруктураРаздела3 = СтруктураРеквизитовФормы.мДанныеОтчета.ПолеТабличногоДокументаРаздел3;
	
	// Добавляем расшифровку для строки 060.
	ОкрП000030006003 = РегламентированнаяОтчетностьКлиентСервер.ОкруглитьЧислоПоФормату(ЭтаФорма, СтруктураРаздела3.П000030006003);
	НазваниеПоказателя = "Раздел 3, строка 060";
	РегламентированнаяОтчетность.ЗарегистрироватьПоказательДляРасшифровки(СписокРасшифровкиПоказателей, "П000030006003",
		НазваниеПоказателя, ОкрП000030006003, "", СтруктураРеквизитовФормы.мСохраненныйДок.Ссылка, "Раздел3", Неопределено, ,
		СчетчикФормулДляРасшифровок);
	
	// Добавляем расшифровку для строки 010.
	ОкрП000030001003 = РегламентированнаяОтчетностьКлиентСервер.ОкруглитьЧислоПоФормату(ЭтаФорма, СтруктураРаздела3.П000030001003);
	НазваниеПоказателя = "Раздел 3, строка 010";
	РегламентированнаяОтчетность.ЗарегистрироватьПоказательДляРасшифровки(СписокРасшифровкиПоказателей, "П000030001003",
		НазваниеПоказателя, ОкрП000030001003, "", СтруктураРеквизитовФормы.мСохраненныйДок.Ссылка, "Раздел3", Неопределено, ,
		СчетчикФормулДляРасшифровок);
		
	СуммаНалогаДолжноБытьВсего = ОкрП000030006003;
	СуммаНалогаПоВсемОКАТО = 0;
	
	// Подсчитываем число заполненых ОКАТО
	КоличествоБлоков = 8;
	ВсегоБлоков = 0;
	ТекущийБлок = 0;
	
	НомерСтраницы = 1;
	
	Для каждого Страница Из СтруктураРеквизитовФормы.мДанныеОтчета.Раздел1 Цикл
		
		Для Индекс = 1 По КоличествоБлоков Цикл
			ИмяПоказателяОКАТО = "П000010002003_" + Индекс;
			КодОКАТО = СокрЛП(Страница.Данные[0].Значение[ИмяПоказателяОКАТО]);
			Если НЕ ПустаяСтрока(КодОКАТО) Тогда 
				ВсегоБлоков = ВсегоБлоков + 1;
			КонецЕсли;
		КонецЦикла;
		
		НомерСтраницы = НомерСтраницы + 1;
		
	КонецЦикла;
	
	НомерСтраницы = 1;
	
	Для каждого Страница Из СтруктураРеквизитовФормы.мДанныеОтчета.Раздел1 Цикл
				
		Для Индекс = 1 По 8 Цикл
			
			ОписаниеКС = "Сумма единого налога на вмененный доход, подлежащая уплате за налоговый период (строка 030 Раздел 1) по данному ОКАТО должна быть равна произведению суммы налога, подлежащей уплате (строка 060 Раздела 3) и результата деления суммы строк 100 всех Разделов 2 по данному ОКАТО и налоговой базы, исчисленной по всем ОКАТО (строка 010 Раздела 3)";
			СчетчикФормулДляРасшифровок = СчетчикФормулДляРасшифровок + 1;
			РегламентированнаяОтчетность.ЗарегистрироватьФормулуДляРасшифровки(СписокРасшифровкиФормулы, ОписаниеКС, 
				, СчетчикФормулДляРасшифровок);
			
			ИмяПоказателяОКАТО = "П000010002003_" + Индекс;
			КодОКАТО = СокрЛП(Страница.Данные[0].Значение[ИмяПоказателяОКАТО]);
			Если ПустаяСтрока(КодОКАТО) Тогда 
				Продолжить;
			КонецЕсли;
			
			ТекущийБлок = ТекущийБлок + 1;
			
			НазваниеПоказателяОКАТО = "Раздел 1, строка 020, страница № " + НомерСтраницы;
			РегламентированнаяОтчетность.ЗарегистрироватьПоказательДляРасшифровки(СписокРасшифровкиПоказателей, ИмяПоказателяОКАТО, НазваниеПоказателяОКАТО, КодОКАТО, "", СтруктураРеквизитовФормы.мСохраненныйДок.Ссылка, "Раздел1", Неопределено, НомерСтраницы, СчетчикФормулДляРасшифровок);
			
			// Зарегистрируем левую часть соотношения.
			ИмяПоказателя = "П000010003003_" + Индекс;
			ЛеваяЧасть = РегламентированнаяОтчетностьКлиентСервер.ОкруглитьЧислоПоФормату(ЭтаФорма, Страница.Данные[0].Значение[ИмяПоказателя]);
			НазваниеПоказателя = "Раздел 1, строка 030, страница № " + НомерСтраницы;
			РегламентированнаяОтчетность.ЗарегистрироватьПоказательДляРасшифровки(СписокРасшифровкиПоказателей, ИмяПоказателя, НазваниеПоказателя, ЛеваяЧасть, "", СтруктураРеквизитовФормы.мСохраненныйДок.Ссылка, "Раздел1", Неопределено, НомерСтраницы, СчетчикФормулДляРасшифровок);
			
			ЛеваяЧастьСтр = Формат(ЛеваяЧасть, "ЧРД=.;ЧН=0");
			
			// Зарегистрируем правую часть соотношения.
			
			СуммаР2Стр100 = 0;
			
			НомерСтраницыР2 = 1;
			
			Для каждого СтраницаР2 Из СтруктураРеквизитовФормы.мДанныеОтчета.Раздел2 Цикл
				
				НомерСтраницыР2 = НомерСтраницыР2 + 1;
				
				КодОКАТОНаСтранице = СокрЛП(СтраницаР2.Данные[0].Значение["П000020003000"]);
				Если ПустаяСтрока(КодОКАТОНаСтранице) ИЛИ КодОКАТО <> КодОКАТОНаСтранице Тогда 
					Продолжить;
				КонецЕсли;
				
				ЗначениеПоказателя = РегламентированнаяОтчетностьКлиентСервер.ОкруглитьЧислоПоФормату(ЭтаФорма, СтраницаР2.Данные[0].Значение.П000020010003);
				СуммаР2Стр100 = СуммаР2Стр100 + ЗначениеПоказателя;
				НазваниеПоказателя = "Раздел 2, строка 100, страница № " + НомерСтраницыР2;
				РегламентированнаяОтчетность.ЗарегистрироватьПоказательДляРасшифровки(СписокРасшифровкиПоказателей, "П000020010003", НазваниеПоказателя, ЗначениеПоказателя, "", СтруктураРеквизитовФормы.мСохраненныйДок.Ссылка, "Раздел2", Неопределено, НомерСтраницыР2, СчетчикФормулДляРасшифровок);
				
				
			КонецЦикла;
			СуммаР2Стр100Стр = Формат(СуммаР2Стр100, "ЧРД=.;ЧН=0");
			
			Если ТекущийБлок = ВсегоБлоков Тогда 
				// Крайний ОКАТО, значит правая часть КС 
				ПраваяЧасть = СуммаНалогаДолжноБытьВсего - СуммаНалогаПоВсемОКАТО;
			Иначе
				// Очередной ОКАТО, вычисляем по формуле КС
				ПраваяЧасть = ?(ОкрП000030001003 = 0, 0, РегламентированнаяОтчетностьКлиентСервер.ОкруглитьЧислоПоФормату(ЭтаФорма, ОкрП000030006003 * СуммаР2Стр100 / ОкрП000030001003));
				СуммаНалогаПоВсемОКАТО = СуммаНалогаПоВсемОКАТО + ПраваяЧасть;
			КонецЕсли;
			
			Если ЛеваяЧасть = ПраваяЧасть Тогда 
				ЭтоОшибка = Ложь;
				ОбщееЧислоБезОшибок = ОбщееЧислоБезОшибок + 1;
			Иначе
				ЭтоОшибка = Истина;
				ОбщееЧислоОшибок = ОбщееЧислоОшибок + 1;
			КонецЕсли;
			
			ПраваяЧастьСтр = Формат(ПраваяЧасть, "ЧРД=.;ЧН=0");
			
			Расшифровка = ЛеваяЧастьСтр + " = " + ОкрП000030006003 + " * " + СуммаР2Стр100Стр + " / "+ ОкрП000030001003
			+ " Итог [" + ПраваяЧастьСтр + "]";
			
			ТекстовоеПредставлениеФормулы = ?(ПустаяСтрока(ТекстФормулы), "", ТекстФормулы + ". ") + ОписаниеКС;
			СтруктураОшибок.Вставить("Описание" + Формат(ОбщееЧислоОшибок + ОбщееЧислоБезОшибок, "ЧГ=0"), "Нарушено соотношение показателей. Возможна арифметическая ошибка в декларации.");
			СтруктураОшибок.Вставить("Расшифровка" + Формат(ОбщееЧислоОшибок + ОбщееЧислоБезОшибок, "ЧГ=0"), Расшифровка);
			СтруктураОшибок.Вставить("НомерФормулыРасшифровки" + Формат(ОбщееЧислоОшибок + ОбщееЧислоБезОшибок, "ЧГ=0"), СчетчикФормулДляРасшифровок);
			СтруктураОшибок.Вставить("ЭтоОшибка" + Формат(ОбщееЧислоОшибок + ОбщееЧислоБезОшибок, "ЧГ=0"), ЭтоОшибка);
			СтруктураОшибок.Вставить("ТекстовоеПредставлениеФормулы" + Формат(ОбщееЧислоОшибок + ОбщееЧислоБезОшибок, "ЧГ=0"), ТекстовоеПредставлениеФормулы);
			
		КонецЦикла;
		
		НомерСтраницы = НомерСтраницы + 1;
		
	КонецЦикла;
	
	СтруктураРезультата.Вставить("КоличествоНайденныхОшибок", ОбщееЧислоОшибок);
	СтруктураРезультата.Вставить("КоличествоНайденныхБезОшибок", ОбщееЧислоБезОшибок);
	СтруктураРезультата.Вставить("СтруктураОшибок", СтруктураОшибок);
	
КонецПроцедуры

&НаСервере
Процедура ПроверкаКС_1_2(СтруктураРезультата)

	ОбщееЧислоОшибок = СтруктураРезультата.КоличествоНайденныхОшибок; // Счетчик учета ошибок.
	ОбщееЧислоБезОшибок = СтруктураРезультата.КоличествоНайденныхБезОшибок; // Счетчик учета проверенных соотношений, не являющиеся ошибками.
	СтруктураОшибок = СтруктураРезультата.СтруктураОшибок;
	
	ИмяРаздела = "Раздел2";
	НазваниеСписка = "Списки2012Кв1";
	НазваниеОбласти = "ВидыУслуг";
	ТекстПроверяемоеСоотношение = "Коды оказываемых услуг по ОКУН (строка 015 Раздела 2) должны соответствовать кодам из Справочника ""Коды услуг ОКУН, в отношении которых может применяться система налогообложения в виде ЕНВД.""";
	ТекстВЗаголовкеКРасшифровке = "Коды оказываемых услуг по ОКУН";
	ПустоеЗначениеЯвляетсяОшибкой = "0";
	
	ВозможныеЗначения = ПолучитьВозможныеЗначения(НазваниеСписка, НазваниеОбласти);

	НомерСтраницы = 1;
	
	// РАЗДЕЛ МНОГОСТРАНИЧНЫЙ
	Для каждого Страница Из СтруктураРеквизитовФормы.мДанныеОтчета[ИмяРаздела] Цикл
		// Регистрируем формулу для каждого экземпляра листа.
		СчетчикФормулДляРасшифровок = СчетчикФормулДляРасшифровок + 1;
		РегламентированнаяОтчетность.ЗарегистрироватьФормулуДляРасшифровки(СписокРасшифровкиФормулы, ТекстПроверяемоеСоотношение,
			, СчетчикФормулДляРасшифровок);
			
		КоличествоСтрокМнЧ = 6;
		КоличествоГраф = 6;
		Показатель = "П00002000150";
		Для Графа = 1 По КоличествоГраф Цикл
			Для Инд = 1 По КоличествоСтрокМнЧ Цикл
				ИндГрафы = Формат(Графа, "ЧГ=0");
				ИндСтр = Формат(Инд, "ЧГ=0");
				Значение = СокрЛП(Страница.Данные[0].Значение[Показатель + Графа + "_" + ИндСтр]);
				Если ПустаяСтрока(Значение) Тогда
					Продолжить;
				КонецЕсли;
				
				СчетчикФормулДляРасшифровок = СчетчикФормулДляРасшифровок + 1;
				РегламентированнаяОтчетность.ЗарегистрироватьФормулуДляРасшифровки(СписокРасшифровкиФормулы, ТекстПроверяемоеСоотношение,
					, СчетчикФормулДляРасшифровок);
				
				ПоказательДляРасш = Показатель + ИндГрафы + "_" + ИндСтр;
				ТекстВЗаголовкеКРасшифровке1 = ТекстВЗаголовкеКРасшифровке + ", страница № " + НомерСтраницы + ", строка № " + Строка(ИндСтр) + ", графа № " + Строка(ИндГрафы);
				ОбработатьРезультатПроверкиВСписке(СписокРасшифровкиПоказателей, ПоказательДляРасш, ТекстВЗаголовкеКРасшифровке1,
					Значение, СтруктураРеквизитовФормы.мСохраненныйДок.Ссылка, ИмяРаздела, ВозможныеЗначения, ОбщееЧислоОшибок,
					ОбщееЧислоБЕЗОшибок, СтруктураОшибок, ТекстПроверяемоеСоотношение, ПустоеЗначениеЯвляетсяОшибкой, НомерСтраницы,
					СчетчикФормулДляРасшифровок);
				
			КонецЦикла;
		КонецЦикла;
		
		НомерСтраницы = НомерСтраницы + 1;
		
	КонецЦикла;
	
	Если ОбщееЧислоОшибок + ОбщееЧислоБезОшибок > 0 Тогда
		СтруктураРезультата.Вставить("КоличествоНайденныхОшибок", ОбщееЧислоОшибок);
		СтруктураРезультата.Вставить("КоличествоНайденныхБезОшибок", ОбщееЧислоБезОшибок);
		СтруктураРезультата.Вставить("СтруктураОшибок", СтруктураОшибок);
	КонецЕсли;

КонецПроцедуры

