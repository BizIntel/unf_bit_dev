		ЭтоОшибка = Ложь;
		ОбщееЧислоБезОшибок = ОбщееЧислоБезОшибок + 1;
	Иначе
		ЭтоОшибка = Истина;
		ОбщееЧислоОшибок = ОбщееЧислоОшибок + 1;
	КонецЕсли;
	
	Расшифровка = ?(ЗначениеЗаполнено(ЗначениеПоказателя), Строка(ЗначениеПоказателя), "« »") + " := ХХХ-ХХХ-ХХХХХХ";
	
