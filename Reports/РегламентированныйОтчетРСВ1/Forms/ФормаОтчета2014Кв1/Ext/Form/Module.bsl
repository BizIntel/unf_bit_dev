	СуммаДоходов = 0;
	СуммаДолейДоходов = 0;
	ЕстьПредопределенныйОКВЭД = Ложь;
	
	НомСтроки = 1;
	Пока ДанныеРаздела.Свойство("П000350000122_" + Формат(НомСтроки, "ЧГ=")) Цикл
		НомерСтроки = Формат(НомСтроки, "ЧГ=");
		
		СтрокаЗаполнена = Ложь;
		ДанныеРаздела.Свойство("П000350000101_" + НомерСтроки, НомерПП);
		ДанныеРаздела.Свойство("П000350000121_" + НомерСтроки, КодДеятельностиПоОКВЭД);
		ДанныеРаздела.Свойство("П000350000122_" + НомерСтроки, ВидЭкономическойДеятельности);
		ДанныеРаздела.Свойство("П000350000103_" + НомерСтроки, Доходы);
		ДанныеРаздела.Свойство("П000350000104_" + НомерСтроки, ДоляДоходов);
		
		СтрокаЗаполнена = ЗначениеЗаполнено(НомерПП) ИЛИ ЗначениеЗаполнено(КодДеятельностиПоОКВЭД)
		              ИЛИ ЗначениеЗаполнено(ВидЭкономическойДеятельности)
		              ИЛИ ЗначениеЗаполнено(Доходы) ИЛИ ЗначениеЗаполнено(ДоляДоходов);
		
		Если СтрокаЗаполнена Тогда
			Если ПустаяСтрока(КодДеятельностиПоОКВЭД) Тогда
				ТекстОписанияОшибки = СтроковыеФункцииКлиентСервер.ПодставитьПараметрыВСтроку(НСтр(
					"ru='Не задан код вида деятельности по ОКВЭД в строке %1!'"), НомерСтроки);
				СообщитьОбОшибкеЗаполнения(
					СтатусПроверки, ТекстОписанияОшибки, ИмяРаздела, "П000350000121_" + НомерСтроки);
			Иначе
				Если НЕ РегламентированнаяОтчетностьКлиентСервер.ОКВЭДСоответствуетТребованиям(
					КодДеятельностиПоОКВЭД) Тогда
					ТекстОписанияОшибки = СтроковыеФункцииКлиентСервер.ПодставитьПараметрыВСтроку(НСтр(
						"ru='Указан некорректный код вида деятельности по ОКВЭД в строке %1!'"), НомерСтроки);
					СообщитьОбОшибкеЗаполнения(
						СтатусПроверки, ТекстОписанияОшибки, ИмяРаздела, "П000350000121_" + НомерСтроки);
				КонецЕсли;
				ЕстьПредопределенныйОКВЭД = ЕстьПредопределенныйОКВЭД ИЛИ ЗначениеСодержитсяВСписке(
					КодДеятельностиПоОКВЭД, СписокПредопределенныхКодовОКВЭД);
			КонецЕсли;
		КонецЕсли;
		
		СуммаДоходов = СуммаДоходов + Доходы;
		СуммаДолейДоходов = СуммаДолейДоходов + ДоляДоходов;
		
		НомСтроки = НомСтроки + 1;
	КонецЦикла;
	
	Если НЕ ЕстьПредопределенныйОКВЭД Тогда
		ТекстОписанияОшибки = СтроковыеФункцииКлиентСервер.ПодставитьПараметрыВСтроку(НСтр(
			"ru='В таблице раздела 3.4 нет ни одной строки с кодом деятельности по ОКВЭД из списка %1!'"),
			СписокПредопределенныхКодовОКВЭД);
		СообщитьОбОшибкеЗаполнения(СтатусПроверки, ТекстОписанияОшибки, ИмяРаздела, "П000350000121_1");
	КонецЕсли;
	
	Если ДанныеРаздела.П000350000204 <> 100 Тогда
		ТекстОписанияОшибки = НСтр("ru='В разделе 3.4 итог по графе 4 должен равняться 100!'");
		СообщитьОбОшибкеЗаполнения(СтатусПроверки, ТекстОписанияОшибки, ИмяРаздела, "П000350000204");
	КонецЕсли;
	
	Если НЕ ЗначениеЗаполнено(ДанныеРаздела.П000350035103) Тогда
		ТекстОписанияОшибки = НСтр("ru='Не указана дата внесения в реестр СМИ в строке 351 раздела 3.4!'");
		СообщитьОбОшибкеЗаполнения(СтатусПроверки, ТекстОписанияОшибки, ИмяРаздела, "П000350035103");
	КонецЕсли;
	
	Если ПустаяСтрока(ДанныеРаздела.П000350035104) Тогда
		ТекстОписанияОшибки = НСтр(
			"ru='Не указан номер записи в реестре аккредитованных организаций в строке 351 раздела 3.4!'");
		СообщитьОбОшибкеЗаполнения(СтатусПроверки, ТекстОписанияОшибки, ИмяРаздела, "П000350035104");
	КонецЕсли;
	
КонецПроцедуры

