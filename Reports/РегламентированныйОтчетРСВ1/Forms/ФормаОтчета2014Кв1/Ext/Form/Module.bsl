		СтруктураРезультата.Вставить("СтруктураОшибок",              СтруктураОшибок);
	КонецЕсли;
	
	// 2(1)
	ИмяПоказателя = "НомерКорректировки";
	НазваниеПоказателя = "Номер корректировки";
