		НазваниеЕдиницыИзмерений = "в млн. рублей";
	Иначе
		КодЕдиницыИзмерения      = "";
		НазваниеЕдиницыИзмерений = "";
	КонецЕсли;
	
	Форма.ТабличныйДокумент.Области.КодЕИ.Значение  = КодЕдиницыИзмерения;
	Форма.ТабличныйДокумент.Области.НазвЕИ.Значение = НазваниеЕдиницыИзмерений;
	
