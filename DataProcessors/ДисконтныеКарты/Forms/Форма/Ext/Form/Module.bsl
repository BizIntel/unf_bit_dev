			КомментарийВидыДисконтныхКарт = "";
		Иначе
			КомментарийВидыДисконтныхКарт = СтрЗаменить(Элемент.ТекущиеДанные.Комментарий, Символы.ПС, " ");
		КонецЕсли;
		
	КонецЕсли;
	
