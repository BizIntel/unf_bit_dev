			ОтветHTTP = ПараметрыСоединения.СоединениеHTTP.ОтправитьДляОбработки(ИмяФайлаЗапроса, "/FtsStat/Stat/List.aspx", ИмяФайлаОтвета, ЗаголовкиHTTP);
