	//
	ИначеЕсли НаимРаздела = "Раздел41" Тогда
		  
		РазделыОтчета[НаимРаздела].Данные = СобратьДанные(ТабличныйДокумент);
		
		ОбновитьСтруктуруМногострочногоРаздела(РазделыОтчета[НаимРаздела].ДанныеДопСтрок, ТабличныйДокумент);
				
	// Обычные разделы.
	//
	Иначе
		
		РазделыОтчета[НаимРаздела].Данные = СобратьДанные(ТабличныйДокумент);
		
	КонецЕсли;	
	
